class Solution {
    public int[] twoSum(int[] nums, int target) {
        int n=nums.length;
        int[] a=new int[2];
        int i=0,j=0,flag=0;
        for(i=0;i<n;i++){
            for(j=0;j<n;j++){
                if(i==j){
                    continue;
                }else if(nums[i]+nums[j]==target){
                    flag=1;
                    break;
                }
            }
            if(flag==1){
                break;
            }
        }
        a[0]=i;
        a[1]=j;
        return a;
    }
}
