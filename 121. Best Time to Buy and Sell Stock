class Solution {
    public int maxProfit(int[] prices) {
        int Profit = 0;
        int minVal=prices[0];
        for(int i=0;i<prices.length;i++){
            minVal = Math.min(prices[i], minVal);
           ////// int profit = prices[i] - minVal;
            Profit = Math.max(Profit, prices[i]-minVal);
        }
        return Profit;
    }
}
